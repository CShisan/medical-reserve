.container {
  width: 100%;
  height: 100%;
  background-color: rgb(245, 245, 245);
}

.call-me-bar {
  margin-top: 20px;
}
.van-cell {
  padding: 5px 20px !important;
  font-size: 13px !important;
  line-height: 13px !important;
  color: rgb(160, 160, 160) !important;
}
.call-me-icon {
  margin-right: 10px;
  vertical-align: middle;
}
.action-bar {
  margin-top: 20px;
}

.action-bar-image {
  margin-top: 15px;
}

.action-bar-bottom {
  display: table;
}

.action-bar-bottom-box {
  display: table-cell;
  vertical-align: middle;
  text-align: center;
}

.action-bar-text {
  display: block;
  margin-top: 10px;
  font-size: 14px;
  font-weight: 650;
  color: rgb(66, 66, 66);
}

.action-bar-desc {
  display: block;
  margin: auto;
  margin-top: 10px;
  width: 80%;
  font-size: 12px;
  color: rgb(150, 150, 150);
}

.custom-map-box {
  border-radius: 10px;
  padding: 10px;
  background-color: #fff;
  margin: 20px 10px 0;
}

.custom-map {
  width: 100%;
  border-radius: 10px;
}
